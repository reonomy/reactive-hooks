{
  "name": "@reonomy/reactive-hooks",
  "version": "1.1.3",
  "description": "RxJS React Hooks Library",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "compile": "tsc --declaration -p ./tsconfig.json --outDir lib/",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext .ts,.tsx --ignore-pattern '**/*.spec.*' --ignore-pattern '**/*.d.ts' src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "pretty-quick --staged",
      "eslint --fix",
      "jest --env=jsdom --findRelatedTests"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reonomy/rx-hooks.git"
  },
  "keywords": [
    "rxjs",
    "react",
    "hooks"
  ],
  "author": "Dmitry Doronin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reonomy/rx-hooks/issues"
  },
  "homepage": "https://github.com/reonomy/rx-hooks#readme",
  "peerDependencies": {
    "react": "^16.4.0",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@testing-library/react": "^9.3.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.8.6",
    "rxjs": "^6.5.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.4.0"
  }
}
